.our-freelancer-content
	.row
		{rows}{row}
		.col-md-4.col-sm-6
			.portfolio-item
				div.portfolio-item-div
					a(href="#")
						img.img-responsive(src="{$profile_image}" alt="")
				h3
					a(href="{$page_url}")
						|{name}Project Name{/}
				.job-position
					|&nbsp; {post}Actress{/}
				ul.contact-details
					{location}
					li
						i.glyphicon.glyphicon-map-marker
						&nbsp;{country}|New York{/}

					{/location}
					{email_wrapper}
					li
						i.glyphicon.glyphicon-envelope
						|  &nbsp;{emails_str}angelina@jolie.com{/}
					{/email_wrapper}
					{contact_wrapper}
					li
						i.glyphicon.glyphicon-phone
						|  &nbsp;{contacts_str}(329) 239-3342{/}
					{/contact_wrapper}
			{design_wrapper}
			.row.freelancer-item-spec
				.col-md-12
					a(href='{$page_url}')
						span Design: 
						span &nbsp;
						span.value {design_count}783{/}
			{/design_wrapper}
			{sales_wrapper}
			.row.freelancer-item-spec
				.col-md-12
					a(href='#')
						span Sales Design" 
						span &nbsp;
						span.value {sales_design}0{/}
			{/sales_wrapper}		
						
            
